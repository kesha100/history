{
  "canisters": {
    "icp_ledger_canister": {
        "type": "custom",
        "candid": "https://raw.githubusercontent.com/dfinity/ic/30bf45e80e6b5c1660cd12c6b554d4f1e85a2d11/rs/rosetta-api/icp_ledger/ledger.did",
        "wasm": "https://download.dfinity.systems/ic/30bf45e80e6b5c1660cd12c6b554d4f1e85a2d11/canisters/ledger-canister.wasm.gz",
        "remote": {
            "id": {
                "ic": "ryjl3-tyaaa-aaaaa-aaaba-cai"
            }
        }
    },
    
    "history_backend": {
      "declarations": {
        "node_compatibility": true
      },
      "main": "src/history_backend/main.mo",
      "type": "motoko"
    },
    "history_frontend": {
      "dependencies": [
        "history_backend"
      ],
      "frontend": {
        "entrypoint": "src/history_frontend/dist/index.html"
      },
      "source": ["src/history_frontend/dist/"],
      "type": "assets",
      "workspace": "history_frontend"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://mainnet.dfinity.network"
      ],
      "type": "persistent"
    }
  },

  "defaults": {
    "build": {
      "args": "",
      "packtool": "mops sources"
    }
  },
  "output_env_file": ".env",
  "version": 1
}